node('mac') {

    stage('cleanup') {
        deleteDir()
    }

    stage('Checkout') {
        def gitScm = checkout scm

        gitScm.each { k,v ->
            echo "key: ${k}, value: ${v}"
        }

        def gitUrl = gitScm.GIT_URL

        echo "Git URL: ${gitUrl}"

        def repos = gitScm.getUserRemoteConfigs()
        repos.each { repo ->
            echo repo.getName()
            echo repo.getUrl()
            echo repo.toString()
        }
    }
}